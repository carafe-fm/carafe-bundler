<html lang="en">
<head>
    <script>
        function bold(targetC) {
            var theDiv = document.getElementById(targetC);
            theDiv.innerHTML = '<b>' + theDiv.innerHTML + '</b>';
        }
    </script>
</head>

<body onload='bold("message")'>
<div>__title__</div>
<div id="message">__message__</div>
</body>
</html>